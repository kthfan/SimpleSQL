    <string-array name="sql_commands">
        <item>SELECT [columns]… FROM [table_name]; </item>
        <item>SELECT * FROM [table_name]; </item>
        <item>SELECT * FROM sqlite_master; </item>
        <item>SELECT DISTINCT [columns]… FROM [table_name]; </item>
        <item>SELECT [columns]… FROM [table_name] WHERE [condition]; </item>
        <item>SELECT [columns]… FROM [table_name] WHERE NOT [condition]; </item>
        <item>SELECT [columns]… FROM [table_name] WHERE IS NULL; </item>
        <item>SELECT [columns]… FROM [table_name] WHERE [condition1] AND [condition2]; </item>
        <item>SELECT [columns]… FROM [table_name] WHERE [condition1] OR [condition2]; </item>
        <item>SELECT [columns]… FROM [table_name] WHERE [column] [comparison operators] [value]; </item>
        <item>SELECT [columns]… FROM [table_name] WHERE [column] BETWEEN [value1] AND [value2]; </item>
        <item>SELECT [columns]… FROM [table_name] WHERE [column] LIKE [pattern]; </item>
        <item>SELECT [columns]… FROM [table_name] WHERE [column] IN ([column]…); </item>
        <item>SELECT [columns]… FROM [table_name] WHERE [column] IN [SELECT STATEMENT]; </item>
        <item>SELECT * FROM [table_name] ORDER BY [columns]… ASC; </item>
        <item>SELECT * FROM [table_name] ORDER BY [columns]… DESC; </item>
        <item>SELECT [column] AS [alias] FROM [table_name]; </item>
        <item>SELECT [columns]… FROM [table_name] AS [alias]; </item>
        <item>INSERT INTO [table_name] ( [columns]… ) VALUES ( [values1]… ), ( [values2]… ), …; </item>
        <item>UPDATE [table_name] SET [column1] = [value1], [column2] = [value2], … WHERE [condition]; </item>
        <item>DELETE FROM [table_name]; </item>
        <item>DELETE FROM [table_name] WHERE [condition]; </item>
        <item>CREATE TABLE IF NOT EXISTS [table_name] ( [id] INTEGER PRIMARY KEY AUTOINCREMENT, [column] [data type], … ); </item>
        <item>CREATE VIEW [view_name] AS [sql_statement]; </item>
        <item>DROP TABLE [table_name]; </item>
        <item>CASE WHEN [condition1] THEN [result1]\n
    WHEN [condition2] THEN [result2]\n
    ELSE [result]\n
END; </item>
        <item>CREATE PROCEDURE [procedure_name]\n
    AS\n
    [sql_statement]\n
GO;\n </item>
        <item>EXEC [procedure_name]; </item>
        <item>ALTER TABLE [table_name] ADD [column] [data type]; </item>
        <item>ALTER TABLE [old_table] RENAME TO [new_table]; </item>
